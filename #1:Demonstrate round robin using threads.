#Insertion Operation
arr=[]
n=int(input("How many elements you want ? "))
for i in range(n):
    elt=int(input("Enter element # "))
    arr.append(elt)
print("INITIAL ARRAY : ")
for i in range(n):
    print("Array[",i,"]",arr[i])

pos=int(input("Enter position where you want to add : "))
val=int(input("Enter element to be added : "))
j=n
arr.append(0)
n=n+1
while (j>=pos):
    arr[j]=arr[j-1]
    j=j-1
arr[pos]=val
print("After Initial Array : ")
for i in range(n):
    print("Array [",i,"]",arr[i])
